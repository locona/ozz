[
  {
    "id": "resources:oathkeeper:rules:ozz:healthcheck",
    "upstream": {
      "url": "http://host.docker.internal:9999/"
    },
    "match": {
      "url": "http://localhost:4455/sample",
      "methods": [
        "GET"
      ]
    },
    "authenticators": [
      {
        "handler": "oauth2_introspection"
      }
    ],
    "authorizer": {
      "handler": "allow"
    },
    "credentials_issuer": {
      "handler": "id_token",
      "config": {
        "aud": []
      }
    }
  }
]
